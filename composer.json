{
	"name": "orchestra/asset",
	"description": "Asset Component for Orchestra Platform",
	"license": "MIT",
	"homepage": "http://orchestraplatform.com/docs/latest/components/asset/",
	"keywords": ["orchestra-platform", "orchestral", "laravel", "asset"],
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Asset\\" : "src/Asset/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"orchestra/html": "2.1.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"illuminate/filesystem": "5.0.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/asset component outside of Orchestra Platform",
		"orchestra/foundation": "Allow using orchestra/asset with Orchestra Platform",
		"illuminate/filesystem": "Allow using orchestra/asset component outside of Laravel app"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		}
	},
	"minimum-stability": "dev"
}
